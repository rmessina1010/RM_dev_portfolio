=> { return (str + '').trim().length <= max ? true : false; }\n\n\n\n    validate(data = {}, fields = null) {\n        if (!fields) { fields = this.fields; }\n        let field;\n        for (field in fields) {\n            let value = data[field];\n            let tests = fields[field];\n\n            let error = '';\n            tests.forEach(test => {\n                console.log(data)\n                if (this[test.test]) {\n                    let arg = test.test === 'match' ? data[test.arg] : test.arg\n                    if (!this[test.test].call(this, value, arg)) { error += test.err + ' '; }\n                }\n            });\n            this.errors[field] = error ? error : null;\n        }\n        return this.errors;\n    }\n}\n\nexport default validator;","/Users/macg4/Documents/nucamp/NucampFolder/RM_dev_portfolio/src/components/resumePageComponent.js",["70"],"import React from 'react';\nimport { Row, Col, NavItem, NavLink } from 'reactstrap';\nimport { ResNav, Summary, Skills, WorkHist, Projects, Awards, Education } from './resumeComponents';\nimport resData from '../shared/resumeData';\nimport '../css/resumeStyles.css';\n\nresData.experience.sort((a, b) => a.endDate < b.endDate);\n\nfunction Resume() {\n    let resumeSections = [];\n\n    let navItems = Array.isArray(resData.sections) ? resData.sections.map(\n        section => {\n            let oldLen = resumeSections.length;\n            switch (section.type) {\n                case 'summ':\n                    if (resData.summary) {\n                        resumeSections.push(\n                            <Summary secTitle={section.title} id={section.id} em={resData.summary.em}>{resData.summary.text}</Summary>\n                        )\n                    }\n                    break;\n                case 'tools':\n                case 'codes':\n                case 'lings':\n                case 'libs':\n                    if (resData.skills && resData.skills[section.type]) {\n                        resumeSections.push(\n                            <Skills secTitle={section.title} id={section.id} items={resData.skills[section.type]} />\n                        )\n                    }\n                    break;\n                case 'hist':\n                    if (resData.experience) {\n                        resumeSections.push(\n                            <WorkHist secTitle={section.title} id={section.id} items={resData.experience} />\n                        )\n                    }\n                    break;\n                case 'awards':\n                    if (resData.awards) {\n                        resumeSections.push(\n                            <Awards secTitle={section.title} id={section.id} items={resData.awards} />\n                        )\n                    }\n                    break;\n                case 'edu':\n                    if (resData.edu) {\n                        resumeSections.push(\n                            <Education secTitle={section.title} id={section.id} items={resData.edu} />\n                        )\n                    }\n                    break;\n                case 'projs':\n                    if (resData.projs) {\n                        resumeSections.push(\n                            <Projects secTitle={section.title} id={section.id} items={resData.projs} />\n                        )\n                    }\n                    break;\n            }\n\n            return resumeSections.length > oldLen ? (<NavItem tag=\"li\"><NavLink href={'#' + section.id}>{section.name || section.title}</NavLink></NavItem>) : null;\n        }) : [];\n\n    return (\n        <div className=\"container-fluid container-lg\">\n            <Row>\n                <Col xs=\"12\" md=\"2\" className=\"res-sidenav\" >\n                    <ResNav children={navItems} dlfile={resData.dlfile || null} title=\"Résumé\" />\n                </Col>\n                <Col xs=\"12\" md=\"10\" >\n                    {resumeSections}\n                </Col>\n            </Row>\n        </div>\n\n    )\n}\n\nexport default Resume;","/Users/macg4/Documents/nucamp/NucampFolder/RM_dev_portfolio/src/components/resumeComponents.js",[],"/Users/macg4/Documents/nucamp/NucampFolder/RM_dev_portfolio/src/shared/resumeData.js",[],"/Users/macg4/Documents/nucamp/NucampFolder/RM_dev_portfolio/src/components/projectsComponent.js",[],"/Users/macg4/Documents/nucamp/NucampFolder/RM_dev_portfolio/src/shared/projectList.js",["71","72","73","74","75","76","77","78","79","80","81","82","83","84"],{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","severity":1,"message":"90","line":2,"column":25,"nodeType":"91","messageId":"92","endLine":2,"endColumn":33},{"ruleId":"93","severity":1,"message":"94","line":22,"column":60,"nodeType":"95","messageId":"96","endLine":22,"endColumn":61,"suggestions":"97"},{"ruleId":"98","severity":1,"message":"99","line":15,"column":13,"nodeType":"100","messageId":"101","endLine":61,"endColumn":14},{"ruleId":"89","severity":1,"message":"102","line":2,"column":7,"nodeType":"91","messageId":"92","endLine":2,"endColumn":9},{"ruleId":"89","severity":1,"message":"103","line":4,"column":7,"nodeType":"91","messageId":"92","endLine":4,"endColumn":9},{"ruleId":"89","severity":1,"message":"104","line":6,"column":7,"nodeType":"91","messageId":"92","endLine":6,"endColumn":9},{"ruleId":"89","severity":1,"message":"105","line":8,"column":7,"nodeType":"91","messageId":"92","endLine":8,"endColumn":11},{"ruleId":"89","severity":1,"message":"106","line":12,"column":7,"nodeType":"91","messageId":"92","endLine":12,"endColumn":16},{"ruleId":"89","severity":1,"message":"107","line":13,"column":7,"nodeType":"91","messageId":"92","endLine":13,"endColumn":11},{"ruleId":"89","severity":1,"message":"108","line":14,"column":7,"nodeType":"91","messageId":"92","endLine":14,"endColumn":11},{"ruleId":"89","severity":1,"message":"109","line":15,"column":7,"nodeType":"91","messageI