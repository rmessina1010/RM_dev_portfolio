tr => { return /[A-Za-z]{1,}(?:\\s[A-Za-z]{1,})*/.test(str); }\n\n    match = (str1, str2) => { return (str1 === str2) ? true : false; }\n\n    required = str => { console.log(str); return this.minLen(str); }\n\n    requiredTrue = el => { return el ? true : false; }\n\n    minLen = (str, min = 1) => { return (str + '').trim().length >= min ? true : false; }\n\n    maxLen = (str, max = 1) => { return (str + '').trim().length <= max ? true : false; }\n\n\n\n    validate(data = {}, fields = null) {\n        if (!fields) { fields = this.fields; }\n        let field;\n        for (field in fields) {\n            let value = data[field];\n            let tests = fields[field];\n\n            let error = '';\n            tests.forEach(test => {\n                console.log(data)\n                if (this[test.test]) {\n                    let arg = test.test === 'match' ? data[test.arg] : test.arg\n                    if (!this[test.test].call(this, value, arg)) { error += test.err + ' '; }\n                }\n            });\n            this.errors[field] = error ? error : null;\n        }\n        return this.errors;\n    }\n}\n\nexport default validator;","/Users/macg4/Documents/nucamp/NucampFolder/RM_dev_portfolio/src/components/resumePageComponent.js",["66"],"import React from 'react';\nimport { Row, Col, NavItem, NavLink } from 'reactstrap';\nimport { ResNav, Summary, Skills, WorkHist, Projects, Awards, Education } from './resumeComponents';\nimport resData from '../shared/resumeData';\nimport '../css/resumeStyles.css';\n\nresData.experience.sort((a, b) => a.endDate < b.endDate);\n\nfunction Resume() {\n    let resumeSections = [];\n\n    let navItems = Array.isArray(resData.sections) ? resData.sections.map(\n        section => {\n            let oldLen = resumeSections.length;\n            switch (section.type) {\n                case 'summ':\n                    if (resData.summary) {\n                        resumeSections.push(\n                            <Summary secTitle={section.title} id={section.id} em={resData.summary.em}>{resData.summary.text}</Summary>\n                        )\n                    }\n                    break;\n                case 'tools':\n                case 'codes':\n                case 'lings':\n                case 'libs':\n                    if (resData.skills && resData.skills[section.type]) {\n                        resumeSections.push(\n                            <Skills secTitle={section.title} id={section.id} items={resData.skills[section.type]} />\n                        )\n                    }\n                    break;\n                case 'hist':\n                    if (resData.experience) {\n                        resumeSections.push(\n                            <WorkHist secTitle={section.title} id={section.id} items={resData.experience} />\n                        )\n                    }\n                    break;\n                case 'awards':\n                    if (resData.awards) {\n                        resumeSections.push(\n                            <Awards secTitle={section.title} id={section.id} items={resData.awards} />\n                        )\n                    }\n                    break;\n                case 'edu':\n                    if (resData.edu) {\n                        resumeSections.push(\n                            <Education secTitle={section.title} id={section.id} items={resData.edu} />\n                        )\n                    }\n                    break;\n                case 'projs':\n                    if (resData.projs) {\n                        resumeSections.push(\n                            <Projects secTitle={section.title} id={section.id} items={resData.projs} />\n                        )\n                    }\n                    break;\n            }\n\n            return resumeSections.length > oldLen ? (<NavItem tag=\"li\"><NavLink href={'#' + section.id}>{section.name || section.title}</NavLink></NavItem>) : null;\n        }) : [];\n\n    return (\n        <div className=\"container-fluid container-lg\">\n            <Row>\n                <Col xs=\"12\" md=\"2\" className=\"d-none d-md-block res-sidenav\" >\n                    <ResNav children={navItems} dlfile={resData.dlfile || null} title=\"Résumé\" />\n                </Col>\n                <Col xs=\"12\" md=\"10\" >\n                    {resumeSections}\n                </Col>\n            </Row>\n        </div>\n\n    )\n}\n\nexport default Resume;","/Users/macg4/Documents/nucamp/NucampFolder/RM_dev_portfolio/src/components/resumeComponents.js",[],"/Users/macg4/Documents/nucamp/NucampFolder/RM_dev_portfolio/src/shared/resumeData.js",[],"/Users/macg4/Documents/nucamp/NucampFolder/RM_dev_portfolio/src/components/projectsComponent.js",["67","68","69","70"],{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","severity":1,"message":"76","line":2,"column":25,"nodeType":"77","messageId":"78","endLine":2,"endColumn":33},{"ruleId":"75","severity":1,"message":"79","line":9,"column":10,"nodeType":"77","messageId":"78","endLine":9,"endColumn":17},{"ruleId":"80","severity":1,"message":"81","line":22,"column":60,"nodeType":"82","messageId":"83","endLine":22,"endColumn":61,"suggestions":"84"},{"ruleId":"85","severity":1,"message":"86","line":15,"column":13,"nodeType":"87","messageId":"88","endLine":61,"endColumn":14},{"ruleId":"75","severity":1,"message":"89","line":2,"column":10,"nodeType":"77","messageId":"78","endLine":2,"endColumn":13},{"ruleId":"75","severity":1,"message":"90","line":2,"column":15,"nodeType":"77","messageId":"78","endLine":2,"endColumn":18},{"ruleId":"75","severity":1,"message":"91","line":2,"column":36,"nodeType":"77","messageId":"78","endLine":2,"endColumn":44},{"ruleId":"75","severity":1,"message":"92","line":2,"column":55,"nodeType":"77","messageId":"78","endLine":2,"endColumn":61},"no-native-reassign",["93"],"no-negated-in-lhs",["94"],"no-unused-vars","'Red